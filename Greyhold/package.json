{
  "name": "greygold-ranch",
  "version": "1.0.0",
  "description": "A dynamic and professional website for Greygold Ranch, showcasing goat cheese, goat soap, and eggs for sale.",
  "main": "src/app.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "webpack-dev-server --port 3000 --open",
    "dev:backend": "node server.js --port 5000",
    "dev2": "webpack serve --mode development --open --port 5001"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "babel-loader": "^8.2.3",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^9.1.2",
    "css-loader": "^6.5.1",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^3.3.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.6.0"
  },
  "keywords": [
    "goat",
    "farm",
    "ecommerce",
    "react",
    "website"
  ],
  "author": "Your Name",
  "license": "MIT"
}
